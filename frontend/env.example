# API & Socket.io Server URL (required)
# Example: https://api.smartattendance.com or http://localhost:4000
VITE_API_URL=

# Chatbot Configuration (optional)
# Enable/disable chatbot feature (default: true)
VITE_CHATBOT_ENABLED=true

# RAG Service URL for AI chatbot (optional)
# Example: https://rag-service.smartattendance.com or http://localhost:8001
VITE_RAG_SERVICE_URL=

# Face Registration Configuration (optional)
# Minimum number of images required for face registration (default: 5)
VITE_MIN_REGISTRATION_IMAGES=5

# Maximum number of images allowed for face registration (default: 10)
VITE_MAX_REGISTRATION_IMAGES=10